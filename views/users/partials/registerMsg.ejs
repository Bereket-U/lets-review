<% if (typeof messages != 'undefined') {%> <%messages.forEach(function(msg){%>

<div
  class="alert alert-danger alert-dismissible fade show text-center"
  role="alert"
>
  <%= msg.message %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>

<% });}%> <%if( error != ''){%>

<div
  class="alert alert-danger alert-dismissible fade show text-center"
  role="alert"
>
  <%= error%>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>

<%}%>
